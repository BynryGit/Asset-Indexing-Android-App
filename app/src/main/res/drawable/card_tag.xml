<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="40dp" />
            <solid android:color="#5EB888" />
            <corners android:radius="0dp"/>
        </shape>
    </item>
    <item
        android:top="-25dp"
        android:bottom="32dp"
        android:left="-90dp"
        android:right="75dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#000000" />
            </shape>
        </rotate>
    </item>
    <item
        android:top="32dp"
        android:bottom="-25dp"
        android:left="75dp"
        android:right="-90dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#000000" />
            </shape>
        </rotate>
    </item>
</layer-list>